  "author": "Anand Chowdhary <mail@anandchowdhary.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stethoscope-js/action/issues"
  },
  "homepage": "https://github.com/stethoscope-js/action#readme",
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.10",
    "@zeit/ncc": "^0.22.3",
    "jest": "^26.4.0",
    "semantic-release": "^17.1.1",
    "semantic-release-gitmoji": "^1.3.4",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
